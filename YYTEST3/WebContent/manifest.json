{
	"_version": "1.0.0",
	"sap.app": {
		"id": "sap.ZG001.simpleReport3",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "proxy/sap/opu/odata/sap/ZTEST02_SRV",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
					
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.38.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.38.0"
				},
				"sap.m": {
					"minVersion": "1.38.0"
				},
				"sap.ui.comp": {
					
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ZG001.simpleReport3.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true
			}
		},
		"rootView": {
			"viewName": "sap.ZG001.simpleReport3.view.App",
			"type": "XML"
		},
		"routing": {
			"config": {
				"viewType": "XML",
				"viewPath": "sap.ZG001.simpleReport3.view",
				"targetAggregation": "pages",
				"clearTarget": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "ZZmain",
					"view": "ZZmain",
					"targetAggregation": "pages",
					"targetControl": "fioriContent"
				}
			]
		}
	}
}