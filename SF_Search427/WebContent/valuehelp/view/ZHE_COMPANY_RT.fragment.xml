<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form"
	xmlns:sap.ui.layout="sap.ui.layout">
	<Table id="ZHE_COMPANY_TBL" items="{null>/}">
		<customData>
			<core:CustomData key="sap-ui-fastnavgroup" value="false"
				writeToDom="false" />
		</customData>
		<columns>
			<Column>
				<VBox>
					<Text text="{i18n>ZF_LABEL_CompCd}" />
				</VBox>
			</Column>
			<Column>
				<VBox>
					<Text text="{i18n>ZF_LABEL_CompNm}" />
				</VBox>
			</Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<ObjectIdentifier title="{result>ownerCompanyCode}" />
					<ObjectIdentifier title="{result>ownerCompanyName}" />
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>