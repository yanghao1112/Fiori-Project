{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "sap.ui.demo.wt",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"mainService": {
				"uri": "proxy/sap/opu/odata/sap/ZTEST_18_01_SRV",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "sap.ui.demo.wt.view.App",
			"type": "XML"
		},
		"routing": {
			"config": {
				"viewType": "XML",
				"viewPath": "sap.ui.demo.wt.view",
				"targetAggregation" : "pages",
				"clearTarget" : true
			},
			"routes": [
				{
					"pattern": "",
					"name": "ZZmain",
					"view": "ZZmain",
					"targetAggregation": "pages",
					"targetControl": "AppControl"
				}
			]
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {
					
				},
				"sap.m": {
					
				},
				"sap.ui.layout": {
					
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.wt.i18n.i18n"
				}
			},
			"list": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "data/data.json"
			}
		}
	}
}