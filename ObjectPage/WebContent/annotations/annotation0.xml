				<Annotation Term="UI.SelectionFields">
					<Collection>
						<PropertyPath>OrderNo</PropertyPath>
					</Collection>
				</Annotation>
				
				<!-- List in List Report Page -->
				<Annotation Term="UI.LineItem">
