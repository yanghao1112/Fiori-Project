
<mvc:View controllerName="sap.ui.test.cache.controller.ZZmain"
	xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	xmlns:unified="sap.ui.unified" xmlns:fb="sap.ui.comp.smartfilterbar"
	xmlns:table="sap.ui.table" xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" height="100%">
       <Page title="Home Page">							
          <content>							
             <Panel							
                headerText="UI5 Panel">							
                <content>							
                   <Button							
                   text="{/buttonText}"							
                   press="onShowHello"/>							
                   <Input							
                   value="{/recipient/name}"							
                   description="description"/>							
                </content>							
              </Panel>							
              <List							
              headerText="Fruits"							
              items="{/listData}" >							
                 <items>							
                    <ObjectListItem							
                    title="{Quantity} x {ProductName}"/>							
                 </items>							
              </List>							
          </content>							
       </Page>	
</mvc:View>